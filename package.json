{
  "name": "move-in",
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "user:dev": "yarn workspace @move-in/move-in dev",
    "user:build": "yarn workspace @move-in/move-in build",
    "manager:dev": "yarn workspace @move-in/move-in-manager dev",
    "manager:build": "yarn workspace @move-in/move-in-manager build",
    "design-system:storybook": "yarn workspace @move-in/design-system storybook",
    "codegen": "yarn workspaces foreach -A run codegen"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0"
  },
  "resolutions": {
    "esbuild": "0.19.8"
  }
}
